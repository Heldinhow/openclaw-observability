# Server Configuration
PORT=3000
NODE_ENV=development

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379

# OpenCode Storage
STORAGE_PATH=/root/.local/share/opencode/storage

# Cache Configuration
CACHE_TTL_SECONDS=300

# Observability Integration
OBSERVABILITY_URL=http://localhost:8080/logs
TASK_TRACKING_URL=http://localhost:8080/tasks

# Log Streaming Configuration
LOG_FILE_PATH=/tmp/openclaw/openclaw.log
LOG_STREAM_MAX_CONNECTIONS=100
LOG_CACHE_TTL=60
